<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias type="com.goodtaste.www.user.UserVO" alias="user"></typeAlias>
		<typeAlias type="com.goodtaste.www.store.StoreVO" alias="store"></typeAlias>
		<typeAlias type="com.goodtaste.www.menu.MenuVO" alias="menu"></typeAlias> 
	</typeAliases>

	<!-- Sql Mapper 설정  -->
	<mappers>
		<mapper resource="mappings/user-mapping.xml" />
		<mapper resource="mappings/store-mapping.xml"/>
	</mappers>
</configuration>

